{
    "family": "{{ECR_FAAMILY}}", 
    "executionRoleArn": "arn:aws:iam::005876332748:role/e2e_test_runner",
    "networkMode": "awsvpc", 
    "containerDefinitions": [
        {
            "name": "e2e_{{CUSTOMER_NAME}}_{{PRODUCT_NAME}}_task", 
            "image": "005876332748.dkr.ecr.eu-central-1.amazonaws.com/e2e_{{CUSTOMER_NAME}}_{{PRODUCT_NAME}}:latest", 
            "essential": true, 
            "entryPoint": [
                "/bin/sh"
            ], 
            "command": [
                "/robot/runRobotAndNotify.sh",
                "cases"
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "secretOptions": null,
                "options": {
                  "awslogs-group": "/ecs/e2e_{{CUSTOMER_NAME}}_{{PRODUCT_NAME}}",
                  "awslogs-region": "eu-central-1",
                  "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ], 
    "requiresCompatibilities": [
        "FARGATE"
    ], 
    "cpu": "256", 
    "memory": "512"
}
